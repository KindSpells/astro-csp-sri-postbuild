{
	"name": "@kindspells/astro-sri-csp",
	"version": "0.2.0",
	"description": "An Astro plugin to compute and inject SRI hashes for script and style tags",
	"private": false,
	"type": "module",
	"main": "main.mjs",
	"types": "main.d.ts",
	"exports": {
		"types": "./main.d.ts",
		"default": "./main.mjs"
	},
	"files": ["main.d.ts", "main.mjs"],
	"scripts": {
		"format": "biome format --write .",
		"lint": "pnpm run lint:biome && pnpm run lint:tsc",
		"lint:biome": "biome lint .",
		"lint:tsc": "tsc -p ."
	},
	"keywords": [
		"astro",
		"astro-component",
		"astro-integration",
		"code-generation",
		"csp",
		"content-security-policy",
		"security",
		"sri",
		"subresource-integrity",
		"withastro"
	],
	"author": "KindSpells Labs S.L.",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@types/node": "^20.11.17",
		"astro": "^4.3.5",
		"typescript": "^5.3.3"
	},
	"repository": "github:KindSpells/astro-sri-csp",
	"funding": [
		{
			"type": "opencollective",
			"url": "https://opencollective.com/kindspells-labs"
		},
		{
			"type": "individual",
			"url": "https://ko-fi.com/coderspirit"
		}
	],
	"packageManager": "pnpm@8.15.1",
	"engines": {
		"node": ">= 18.0.0"
	}
}
